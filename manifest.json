{
	"manifest_version": 2,

	"name":"Summarify",
	"description":"Summaries contents of a newspaper article.",
	"version": "1.0",
	"permissions": [
	"cookies",
	"http://*/*",
	"https://*/*"
	],
	
	"browser_action": {
		"default_icon": "summary.png",
		"default_popup": "summarify.html"
		},
		"content_scripts": [ {
			"js": ["jquery-2.2.2.min.js","summarify.js" ],
			"matches": [ "http://*/*", "https://*/*"]
			}]
		}